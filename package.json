{
  "name": "telemedcare-v11-modular",
  "version": "11.0.0",
  "description": "TeleMedCare V11.0 Modular - Enterprise Lead Management System",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev dist --d1=telemedcare-leads --local --ip 0.0.0.0 --port 3000",
    "dev:vite": "vite",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "./scripts/deploy-production.sh",
    "deploy:test": "wrangler pages deploy dist --project-name telemedcare-test --env test",
    "deploy:staging": "wrangler pages deploy dist --project-name telemedcare-staging --env staging",
    "env:create:production": "./scripts/deploy-production.sh",
    "env:create:test": "./scripts/create-test-environment.sh",
    "env:clone": "./scripts/clone-environment.sh",
    "env:clone:prod": "./scripts/clone-environment.sh production false",
    "env:clone:dev": "./scripts/clone-environment.sh development true",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl http://localhost:3000",
    "test:health": "curl http://localhost:3000/api/system/health",
    "test:functional": "curl -X POST http://localhost:3000/api/test/functional/run",
    "test:stress": "curl -X POST http://localhost:3000/api/test/stress/start -d '{\"assistiti_count\":10}' -H 'Content-Type: application/json'",
    "db:migrate:local": "wrangler d1 migrations apply telemedcare-leads --local",
    "db:migrate:test": "wrangler d1 migrations apply telemedcare_test_01",
    "db:migrate:staging": "wrangler d1 migrations apply telemedcare_staging",
    "db:migrate:prod": "wrangler d1 migrations apply telemedcare_database",
    "db:seed": "wrangler d1 execute telemedcare-leads --local --file=./seed.sql",
    "db:seed:test": "wrangler d1 execute telemedcare_test_01 --file=./seed-test.sql",
    "db:reset": "rm -rf .wrangler/state/v3/d1 && npm run db:migrate:local && npm run db:seed",
    "db:console:local": "wrangler d1 execute telemedcare-leads --local",
    "db:console:test": "wrangler d1 execute telemedcare_test_01",
    "db:console:staging": "wrangler d1 execute telemedcare_staging",
    "db:console:prod": "wrangler d1 execute telemedcare_database",
    "docs:init": "curl -X POST http://localhost:3000/api/docs/initialize",
    "docs:generate": "curl -X POST http://localhost:3000/api/docs/generate"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "docx": "^9.5.1",
    "hono": "^4.9.9"
  },
  "devDependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "md-to-pdf": "^5.2.5",
    "playwright": "^1.58.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
